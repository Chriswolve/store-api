{
  "version": 2,
  "builds": [
    {
      "src": "api/src/frameworks/http/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui/(.*)",
      "dest": "api/dist/swagger/swagger-ui.css"
    },
    {
      "src": "/(.*)",
      "dest": "api/src/frameworks/http/server.js"
    }
  ]
}
