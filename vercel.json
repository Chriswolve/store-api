{
  "version": 2,
  "headers": [
    {
      "source": "/api/v1/docs/swagger-ui.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/src/frameworks/http/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/api/public/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/src/frameworks/http/server.js"
    }
  ]
}
