{
  "version": 2,
  "builds": [
    {
      "src": "api/src/frameworks/http/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/dist/swagger/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/src/frameworks/http/server.js"
    }
  ]
}
